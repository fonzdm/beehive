nfs:
    server: 192.168.1.233
    path: /k8s-nfs
    pathPattern: "${namespace}-${pvcName}"
storageClass:
    name: nfs-client
    defaultClass: true
    accessModes: ReadWriteMany
    reclaimPolicy: Retain
    volumeBindingMode: Immediate
    allowVolumeExpansion: true
    mountOptions:
    - nfsvers=4
    - hard
    - noatime
    - actimeo=0
